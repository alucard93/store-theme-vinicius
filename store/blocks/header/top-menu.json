{
  "flex-layout.row#2-desktop": {
    "title": "Seção ",
    "props": {
      "blockClass": "topbar",
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#2-desktop-col-1",
      "flex-layout.row#2-desktop-col-2"
    ]
  },

  "flex-layout.col#2-desktop-col-1": {
    "props": {
      "blockClass": "topbar"
    },
    "children": ["vtex.menu@2.x:menu#top-menu"]
  },

  "vtex.menu@2.x:menu#top-menu": {
    "props": {
      "blockClass": "topbar"
    },
    "children": ["menu-item#quem-somos", "menu-item#trocas-e-devolucoes"]
  },
  "menu-item#quem-somos": {
    "props": {
      "blockClass": "topbar",
      "id": "menu-item#quem-somos",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/quem-somos",
        "noFollow": false,
        "tagTitle": "Quem somos",
        "text": "Quem somos"
      }
    }
  },

  "menu-item#trocas-e-devolucoes": {
    "props": {
      "blockClass": "topbar",
      "id": "menu-item#trocas-e-devolucoes",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/trocas-e-devolucoes",
        "noFollow": false,
        "tagTitle": "Trocas e Devoluções",
        "text": "Trocas e Devoluções"
      }
    }
  },

  "flex-layout.row#2-desktop-col-2": {
    "props": {
      "blockClass": "topbar",
      "colSizing": "auto"
    },
    "children": ["login", "minicart.v2"]
  },

  "login": {
    "props": {
      "iconLabel": "Entre ou cadastre-se",
      "showIconProfile": true,
      "optionsTitle": "Acesse sua conta",
      "emailAndPasswordTitle": "Login",
      "accessCodeTitle": "Receba sua chave por e-mail",
      "emailPlaceholder": "E-mail",
      "passwordPlaceholder": "Senha",
      "accessCodePlaceholder": "Insira o código de acesso",
      "showPasswordVerificationIntoTooltip": true
    }
  }
}
